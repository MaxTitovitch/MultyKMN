<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="oval"  >
            <corners android:radius="10dip" />
            <stroke android:width="3dip" android:height="3dip" android:color="#525252" />
            <gradient android:angle="270" android:startColor="#ffffff" android:endColor="#B9B9B9" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="oval"  >
            <corners android:radius="10dip" />
            <stroke android:width="3dip" android:height="3dip" android:color="#525252" />
            <gradient android:angle="270" android:startColor="#ffffff" android:endColor="#B9B9B9" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval"  >
            <corners android:radius="10dip" />
            <stroke android:width="2dip" android:height="3dip" android:color="#525252" />
            <gradient android:angle="270" android:startColor="#ffffff" android:endColor="#B9B9B9" />
        </shape>
    </item>
</selector>
